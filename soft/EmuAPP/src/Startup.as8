 .section .startup.text, "x"

 .global call_user_start
 .extern ovl_SwitchToMode
 .extern spi_flash_Startup
 .extern main_init
 .extern emu_start
  .extern _stack_init

 .set OVL_MODE_EMU,  0
 .set OVL_MODE_INIT, 3

 .balign  4
 .literal_position
 .balign  4

call_user_start!:
{
    movi  a1,_stack_init
    call0 spi_flash_Startup
    movi  a2,OVL_MODE_INIT
    call0 ovl_SwitchToMode
    call0 main_init
    movi  a2,OVL_MODE_EMU
    call0 ovl_SwitchToMode
    call0 emu_start
}
